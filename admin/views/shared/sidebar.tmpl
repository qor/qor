  </div>
  <div class="sidebar-body">
    <div class="qor-menu-container">
      {{if .Admin.EnabledSearchCenter}}
        <div qor-icon-name="Search">
          <span class="qor-global-search--show" data-placeholder="{{t "qor_admin.search_center.hint" "Searchâ€¦"}}" data-action-url="{{search_center_path}}">{{t "qor_admin.search_center.title" "Global Search" }}</span>
        </div>
      {{end}}
      {{render "shared/menu" get_menus}}
    </div>
  </div>
  <div class="sidebar-footer">
    {{t "qor_admin.layout.powered_by" "Powered by <a href=\"http://getqor.com\" target=\"_blank\">QOR</a>"}}
  </div>
</div>
