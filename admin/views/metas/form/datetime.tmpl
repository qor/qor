<div class="qor-field">
  <div class="mdl-textfield mdl-textfield--full-width mdl-js-textfield qor-textfield--static-label">
    <input class="mdl-textfield__input" type="text" id="{{.InputId}}" data-toggle="qor.datepicker" data-text="{&quot;title&quot;: &quot;{{t "Pick a date"}}&quot;, &quot;ok&quot;: &quot;{{t "OK"}}&quot;, &quot;cancel&quot;: &quot;{{t "Cancel"}}&quot;}" name="{{.InputName}}" {{if .Value}}{{if not .Value.IsZero}}value="{{.Value.Format "2006-01-02 15:04"}}"{{end}}{{end}}
      {{if not (has_update_permission .Meta)}} disabled{{end}}>
    <label class="mdl-textfield__label" for="{{.InputId}}">{{dt (printf "%v.attributes.%v" .BaseResource.ToParam .Label) .Label}}</label>
  </div>
</div>
