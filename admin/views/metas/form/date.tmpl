<div class="mdl-textfield mdl-js-textfield mdl-textfield--floating-label">
  <input class="mdl-textfield__input" type="text" id="{{.InputId}}" data-toggle="qor.datepicker" name="{{.InputName}}"{{if .Value}}{{if not .Value.IsZero}} value="{{.Value.Format "2006-01-02"}}"{{end}}{{end}}
    {{if not (has_update_permission .Meta)}} disabled{{end}}>
  <label class="mdl-textfield__label" for="{{.InputId}}">{{dt (printf "%v.attributes.%v" .Base.ToParam .Label) .Label}}</label>
</div>
