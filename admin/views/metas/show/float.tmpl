<div class="qor-field">
  <div class="mdl-textfield mdl-textfield--full-width mdl-js-textfield qor-textfield--static-label">
    <input class="mdl-textfield__input hidden" type="number" step="any" id="{{.InputId}}" name="{{.InputName}}" value="{{.Value}}" {{if not (has_update_permission .Meta)}}disabled{{end}}>

    <label class="mdl-textfield__label" for="{{.InputId}}">{{meta_label .Meta}}</label>
    <span class="qor-field__value">{{.Value}}</span>
  </div>
</div>
